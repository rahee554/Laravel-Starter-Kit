{
    "name": "artflow-studio/starterkit",
    "description": "A beautiful Laravel authentication and admin starter kit with multiple layout options, Bootstrap 5, dark mode support, and Fortify integration",
    "keywords": [
        "laravel",
        "starter-kit",
        "authentication",
        "admin-panel",
        "bootstrap",
        "fortify",
        "dark-mode",
        "layouts"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Artflow Studio",
            "email": "support@artflow-studio.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "laravel/framework": "^11.0",
        "laravel/fortify": "^1.17"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "ArtflowStudio\\StarterKit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ArtflowStudio\\StarterKit\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ArtflowStudio\\StarterKit\\StarterKitServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
